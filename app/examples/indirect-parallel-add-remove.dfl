def f = [X | Y] link X >-> Y

def main = <a |> [X | Y1, Y2]
  use f [X | Y] as F1
  use f [X | Y] as F2
  use f [X | Y] as F3
  use -a [X | Y] as RemA
  use +a [X | Y] as AddA
  link
    X >-> F1.X,
    F1.Y >-> F2.X,
    F2.Y >-> F3.X,
    F3.Y >-> RemA.X,
    RemA.Y >-> Y1,
    F1.Y >-> AddA.X,
    AddA.Y >-> Y2
